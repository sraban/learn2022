# 08/09/2022 - Thursday

git config --global http.proxy http://proxyuser:proxypwd@proxy.server.com:port

git config --global --unset http.proxy
git config --global --unset https.proxy



======================== File renaming in Folder Windows =========================

forfiles /S /M *.doc /C "cmd /c rename @file @fname.txt"
forfiles /S /M *.docx /C "cmd /c rename @file @fname.js"
forfiles /S /M *.csv /C "cmd /c rename @file @fname.jsx"
forfiles /S /M *.xls /C "cmd /c rename @file @fname.json"
forfiles /S /M *.xlsx /C "cmd /c rename @file @fname.sample"
----------------
forfiles /S /M *.txt /C "cmd /c rename @file @fname.doc"
forfiles /S /M *.js /C "cmd /c rename @file @fname.docx"
forfiles /S /M *.jsx /C "cmd /c rename @file @fname.csv"
forfiles /S /M *.json /C "cmd /c rename @file @fname.xls"
forfiles /S /M *.sample /C "cmd /c rename @file @fname.xlsx"


